<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:padding="20dp"
    android:background="#f5f5f5">
    <ImageView android:id="@+id/mainimg"
    	android:layout_alignParentLeft="true"
    	android:layout_alignParentTop="true"
    	android:layout_height="250dp"
    	android:layout_width="100dp"/>
    <Button android:id="@+id/menupantbtn"
    	android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
    	android:layout_toRightOf="@id/mainimg"
    	android:padding="10dp"
    	android:text="Pantekassa"
    	android:textColor="#f5f5f5"
    	android:background="@drawable/custombtn"
    	/>
    <TextView android:id="@+id/cleanertextview"
    	android:layout_width="fill_parent"
    	android:layout_height="wrap_content"
    	android:layout_below="@id/mainimg"
    	android:padding="20dp"
    	android:textSize="14dp"
    	android:textColor="#000000"
    	android:text="Vaskeuke:"/>
</RelativeLayout>
