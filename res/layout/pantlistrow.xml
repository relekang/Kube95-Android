<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent"
	android:layout_height="50dp">
	<TextView android:id="@+id/pantrow_amount"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:textColor="@color/black"
		android:textSize="20dp"
		android:paddingLeft="6dp"
		android:paddingRight="6dp"></TextView>
	<TextView android:id="@+id/pantrow_date"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:textColor="@color/black"
		android:layout_below="@id/pantrow_amount"
		android:layout_alignParentLeft="true"
		android:paddingLeft="6dp"></TextView>
	<TextView android:id="@+id/pantrow_user"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:textColor="@color/black"
		android:layout_below="@id/pantrow_amount"
		android:layout_alignParentRight="true"
		android:paddingRight="6dp"></TextView>
</RelativeLayout>